{
  "manifest_version": 3,
  "name": "Magical Workspace",
  "version": "1.0",
  "author": "mail.aleks.for.work@gmail.com",
  "description": "Simple workspace",
  "browser_specific_settings": {
    "gecko": {
      "id": "{47bce7c4-4737-41fc-8895-22bd3d95b18b}",
      "strict_min_version": "109.0"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "sidebar_action": {
    "default_title": "My workspace",
    "default_panel": "src/html/main.html",
    "browser_style": false
  },
  "homepage_url": "https://github.com/kristal374/Magical_Workspace"
}
